{
  "name": "back-end",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel ./src -d ./build/src --copy-files",
    "build": "npm run clean && npm run build-babel",
    "production": "node ./build/src/app.js",
    "dev": "nodemon --exec ./node_modules/.bin/babel-node ./src/app.js",
    "migrate": "cd src && sequelize-cli db:migrate",
    "migrate:undo": "cd src && sequelize-cli db:migrate:undo:all",
    "seed": "cd src && sequelize-cli db:seed:all",
    "seed:undo": "cd src && sequelize-cli db:seed:undo:all"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.25.7",
    "@babel/runtime": "^7.25.7",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "chalk": "^5.3.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.13",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.11.3",
    "passport": "^0.7.0",
    "passport-google-oauth2": "^0.2.0",
    "qs": "^6.13.1",
    "sequelize": "^6.37.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui": "^5.18.2",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.14.2",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.7",
    "@babel/core": "^7.25.7",
    "@babel/node": "^7.25.7",
    "@babel/preset-env": "^7.25.7",
    "@types/express": "^5.0.0",
    "nodemon": "^3.1.7",
    "sequelize-cli": "^6.6.2"
  }
}
